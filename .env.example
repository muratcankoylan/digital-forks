# OpenRouter (Recommended - single API for all models)
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_APP_NAME=Forks

# Direct API access (optional, bypasses OpenRouter)
# USE_MINIMAX_DIRECT=true
# MINIMAX_API_KEY=your_minimax_api_key
# ANTHROPIC_API_KEY=your_anthropic_api_key

# Local SQLite Database (default: .data/forks.db)
# DATABASE_PATH=./custom/path/to/forks.db

# OpenClaw (webhook authentication for WhatsApp/Telegram)
OPENCLAW_WEBHOOK_SECRET=your_webhook_secret

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
